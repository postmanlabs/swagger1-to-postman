{
  "name": "swagger1-to-postman",
  "version": "1.1.2",
  "description": "Convert a given Swagger spec to Postman spec",
  "main": "index.js",
  "com_postman_plugin": {
    "plugin_type": "importer",
    "name": "SWAGGER 1.0 to Postman",
    "source_format": "SWAGGER1.0",
    "source_format_name": "SWAGGER 1.0"
  },
  "bin": {
    "swagger2postman": "./bin/swagger2postman"
  },
  "dependencies": {
    "uuid": "3.3.2",
    "postman_validator": ">=0.0.4",
    "lodash": "4.17.21",
    "js-yaml": "3.14.1",
    "commander": "2.20.3"
  },
  "engines": {
    "node": ">=16"
  },
  "author": "Postman Labs <help@getpostman.com>",
  "license": "Apache",
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-jsdoc": "3.8.0",
    "eslint-plugin-mocha": "5.2.0",
    "eslint-plugin-security": "1.4.0",
    "expect.js": "0.3.1",
    "mocha": "10.2.0",
    "nyc": "14.1.1"
  },
  "scripts": {
    "test": "./scripts/test.sh",
    "test-unit": "./scripts/test-unit.sh",
    "test-lint": "./scripts/test-lint.sh",
    "coverage": "nyc report --reporter=html --reporter=text mocha"
  }
}
